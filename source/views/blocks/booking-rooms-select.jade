#booking-select-room-package
	.a-room
		.room-packages
			.room-brief
				.name Double Rooms
				.room-photo
					img(src="images/upload/rooms/room1.jpg", alt="room1")
				ul
					li
						span.title Room size:
						span.value 29 sq.m
					li
						span.title Bed:
						span.value 1 Double bed
					li
						span.title View:
						span.value Sea
					li
						span.title Max:
						span.value 2 Adults
				.more
					i.fa.fa-photo.photo(title="Gallery")
					i.fa.fa-video-camera.video(title="Video clip")
					i.fa.fa-users.comment(title="Customer Comments")
			.package
				.package-info
					.outer
						.inner
							.info-wrap
								.package-spec
									.name Bed and Breakfast
									ul.specs
										li 
											i.fa.fa-cutlery
											span Breakfast
										li 
											i.fa.fa-bed
											span Basic bed
								.extra-spec
									ul.specs
										li
											i.fa.fa-cutlery
											span Extra breakfast
											i.fa.fa-check-circle-o.check.uncheck
