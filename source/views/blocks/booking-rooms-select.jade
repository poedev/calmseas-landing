											i.fa.fa-cutlery
											span Extra spa service
											i.fa.fa-check-circle-o.check.uncheck
											i.fa.fa-check-circle.check.checked
							.price
								.value 199.99 $
								.unit USD
								.unit 1 night
								.unit 1 room
						.checking-room
							.available.good 5 available
